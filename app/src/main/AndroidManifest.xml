<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kx.designpatterns">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreatorPattern$SimpleFactoryPattern.SimpleFactoryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CreatorPattern$FactoryMethodPattern.FactoryMethodActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CreatorPattern$AbstractFactoryPattern.AbstractFactoryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CreatorPattern$BuilderPattern.BuilderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CreatorPattern$PrototypePattern.PrototypePatternActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StructuralPattern.AdapterPattern.AdapterActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".StructuralPattern.WrapperPattern.WrapperActivity"></activity>
    </application>

</manifest>